    "node": ">=18"
  },
  "packageManager": "bun@1.2.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@types/bun": "1.2.3",
    "next": "^15.1.7"
  }
}
